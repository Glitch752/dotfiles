{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "opener:allow-open-url",
    "bar:allow-debug-log",
    "bar:allow-niri-request",
    "launcher:allow-rink-query",
    "launcher:allow-symbols-query",
    "shell:default"
  ]
}